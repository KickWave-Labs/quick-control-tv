{
  "extends": ["prettier", "plugin:jest/recommended"],
  "root": true,
  "reportUnusedDisableDirectives": true,
  "plugins": [
    "babel",
    "ft-flow",
    "jest",
    "no-for-of-loops",
    "no-function-declare-after-return",
    "react",
    "react-internal"
  ],
  "parser": "hermes-eslint",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "script"
  },
  "rules": {
    "ft-flow/array-style-complex-type": [0, "verbose"],
    "ft-flow/array-style-simple-type": [0, "verbose"],
    "ft-flow/boolean-style": 2,
    "ft-flow/no-dupe-keys": 2,
    "ft-flow/no-primitive-constructor-types": 2,
    "ft-flow/no-types-missing-file-annotation": 0,
    "ft-flow/no-unused-expressions": 2,
    "no-cond-assign": 0,
    "no-constant-condition": 0,
    "no-control-regex": 0,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 1,
    "no-empty-character-class": 1,
    "no-empty": 0,
    "no-ex-assign": 1,
    "no-extra-boolean-cast": 1,
    "no-func-assign": 2,
    "no-invalid-regexp": 1,
    "no-irregular-whitespace": 1,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-jsdoc": 0,
    "block-scoped-var": 0,
    "complexity": 0,
    "default-case": 0,
    "guard-for-in": 0,
    "no-alert": 0,
    "no-caller": 2,
    "no-case-declarations": 0,
    "no-div-regex": 0,
    "no-else-return": 0,
    "no-empty-pattern": 1,
    "no-eq-null": 0,
    "no-eval": 2,
    "no-extend-native": 1,
    "no-extra-bind": 1,
    "no-fallthrough": 1,
    "no-implicit-coercion": 0,
    "no-implied-eval": 2,
    "no-invalid-this": 0,
    "no-iterator": 0,
    "no-labels": [2, { "allowLoop": true, "allowSwitch": true }],
    "no-lone-blocks": 1,
    "no-loop-func": 0,
    "no-magic-numbers": 0,
    "no-multi-str": 2,
    "no-native-reassign": [2, { "exceptions": ["Map", "Set"] }],
    "no-new-func": 2,
    "no-new": 1,
    "no-new-wrappers": 1,
    "no-octal-escape": 1,
    "no-octal": 1,
    "no-param-reassign": 0,
    "no-process-env": 0,
    "no-proto": 2,
    "no-redeclare": 0,
    "no-return-assign": 0,
    "no-script-url": 2,
    "no-self-compare": 1,
    "no-sequences": 1,
    "no-throw-literal": 2,
    "no-useless-call": 1,
    "no-void": 0,
    "no-warning-comments": 0,
    "no-with": 0,
    "radix": 1,
    "vars-on-top": 0,
    "yoda": 0,
    "init-declarations": 0,
    "no-catch-shadow": 2,
    "no-delete-var": 2,
    "no-label-var": 1,
    "no-shadow-restricted-names": 1,
    "no-undef-init": 0,
    "no-undef": 2,
    "no-undefined": 0,
    "callback-return": 0,
    "global-require": 0,
    "handle-callback-err": 0,
    "no-mixed-requires": 0,
    "no-new-require": 0,
    "no-path-concat": 0,
    "no-process-exit": 0,
    "no-restricted-modules": 0,
    "no-sync": 0,
    "camelcase": [0, { "properties": "always" }],
    "consistent-this": [0, "self"],
    "func-names": 0,
    "func-style": [0, "declaration"],
    "id-length": 0,
    "id-match": 0,
    "max-depth": 0,
    "max-nested-callbacks": 0,
    "max-params": 0,
    "max-statements": 0,
    "new-cap": 0,
    "newline-after-var": 0,
    "no-array-constructor": 2,
    "no-continue": 0,
    "no-inline-comments": 0,
    "no-lonely-if": 0,
    "no-negated-condition": 0,
    "no-nested-ternary": 0,
    "no-new-object": 1,
    "no-plusplus": 0,
    "no-ternary": 0,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 1,
    "one-var": [1, { "initialized": "never" }],
    "operator-assignment": [1, "always"],
    "require-jsdoc": 0,
    "sort-vars": 0,
    "spaced-comment": [0, "always", { "exceptions": ["jshint", "jslint", "eslint", "global"] }],
    "constructor-super": 2,
    "no-class-assign": 1,
    "no-const-assign": 2,
    "no-dupe-class-members": 2,
    "no-this-before-super": 2,
    "object-shorthand": 0,
    "prefer-const": 0,
    "prefer-spread": 0,
    "prefer-reflect": 0,
    "prefer-template": 0,
    "require-yield": 0,
    "babel/generator-star-spacing": 0,
    "babel/new-cap": 0,
    "babel/array-bracket-spacing": 0,
    "babel/object-curly-spacing": 0,
    "babel/object-shorthand": 0,
    "babel/arrow-parens": 0,
    "babel/no-await-in-loop": 0,
    "babel/flow-object-type": 0,
    "react/display-name": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-closing-bracket-location": 0,
    "react/jsx-curly-spacing": 0,
    "react/jsx-equals-spacing": 1,
    "react/jsx-filename-extension": 0,
    "react/jsx-first-prop-new-line": 0,
    "react/jsx-handler-names": 0,
    "react/jsx-indent": 0,
    "react/jsx-indent-props": 0,
    "react/jsx-key": 0,
    "react/jsx-max-props-per-line": 0,
    "react/jsx-no-bind": 0,
    "react/jsx-no-duplicate-props": 2,
    "react/jsx-no-literals": 0,
    "react/jsx-no-target-blank": 0,
    "react/jsx-pascal-case": 0,
    "react/jsx-sort-props": 0,
    "react/jsx-uses-vars": 2,
    "react/no-comment-textnodes": 0,
    "react/no-danger": 0,
    "react/no-deprecated": 0,
    "react/no-did-mount-set-state": 0,
    "react/no-did-update-set-state": 0,
    "react/no-direct-mutation-state": 0,
    "react/no-multi-comp": 0,
    "react/no-render-return-value": 0,
    "react/no-set-state": 0,
    "react/no-string-refs": 0,
    "react/no-unknown-property": 0,
    "react/prefer-es6-class": 0,
    "react/prefer-stateless-function": 0,
    "react/prop-types": 0,
    "react/require-extension": 0,
    "react/require-optimization": 0,
    "react/require-render-return": 0,
    "react/sort-comp": 0,
    "react/sort-prop-types": 0,
    "accessor-pairs": 0,
    "brace-style": [2, "1tbs"],
    "consistent-return": 0,
    "dot-location": [2, "property"],
    "dot-notation": [2, { "allowPattern": "^(error|warn)$" }],
    "eol-last": 2,
    "eqeqeq": [2, "allow-null"],
    "indent": 0,
    "jsx-quotes": [2, "prefer-double"],
    "keyword-spacing": [2, { "after": true, "before": true }],
    "no-bitwise": 0,
    "no-console": 0,
    "no-inner-declarations": [2, "functions"],
    "no-multi-spaces": 2,
    "no-restricted-globals": [2],
    "no-shadow": 2,
    "no-unused-vars": [2, { "args": "none", "ignoreRestSiblings": true }],
    "no-use-before-define": 0,
    "no-useless-concat": 0,
    "quotes": [2, "single", { "avoidEscape": true, "allowTemplateLiterals": true }],
    "space-before-blocks": 2,
    "space-before-function-paren": 0,
    "valid-typeof": [2, { "requireStringLiterals": true }],
    "no-useless-computed-key": 0,
    "no-var": 2,
    "strict": 2,
    "max-len": 0,
    "react/jsx-boolean-value": [2, "always"],
    "react/jsx-no-undef": 2,
    "react/react-in-jsx-scope": 2,
    "react/self-closing-comp": 2,
    "react/jsx-wrap-multilines": [2, { "declaration": false, "assignment": false }],
    "no-for-of-loops/no-for-of-loops": 2,
    "no-function-declare-after-return/no-function-declare-after-return": 2,
    "react-internal/no-primitive-constructors": 2,
    "react-internal/safe-string-coercion": [2, { "isProductionUserAppCode": true }],
    "react-internal/no-to-warn-dev-within-to-throw": 2,
    "react-internal/warning-args": 2,
    "react-internal/no-production-logging": 2
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "globals": {
    "$Call": "readonly",
    "$ElementType": "readonly",
    "$Flow$ModuleRef": "readonly",
    "$FlowFixMe": "readonly",
    "$Keys": "readonly",
    "$NonMaybeType": "readonly",
    "$PropertyType": "readonly",
    "$ReadOnly": "readonly",
    "$ReadOnlyArray": "readonly",
    "$ArrayBufferView": "readonly",
    "$Shape": "readonly",
    "bigint": "readonly",
    "BigInt": "readonly",
    "BigInt64Array": "readonly",
    "BigUint64Array": "readonly",
    "Class": "readonly",
    "ClientRect": "readonly",
    "ConsoleTask": "readonly",
    "EventListener": "readonly",
    "Iterable": "readonly",
    "Iterator": "readonly",
    "IteratorResult": "readonly",
    "React$Component": "readonly",
    "React$Node": "readonly",
    "__DEV__": "readonly",
    "__TEST__": "readonly",
    "AsyncIterator": "readonly",
    "SyntheticEvent": "readonly",
    "Thenable": "readonly",
    "TaskController": "readonly",
    "spyOnDev": "readonly",
    "globalThis": "readonly"
  }
}
